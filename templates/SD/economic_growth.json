                {
                  "type": "Stock",
                  "subtype": "Normal",
                  "name": "capital_stock",
                  "subscripts": [],
                  "ast": {
                    "kind": "INTEG",
                    "expression": "INTEG(investment_rate - depreciation_rate, 1000)"
                  }
                },
                {
                  "type": "Flow",
                  "subtype": "Normal",
                  "name": "investment_rate",
                  "subscripts": [],
                  "ast": {
                    "kind": "EQUATION",
                    "expression": "gdp * savings_rate"
                  }
                },
                {
                  "type": "Flow",
                  "subtype": "Normal",
                  "name": "depreciation_rate",
                  "subscripts": [],
                  "ast": {
                    "kind": "EQUATION",
                    "expression": "capital_stock * depreciation_fraction"
                  }
                },
                {
                  "type": "Auxiliary",
                  "subtype": "Normal",
                  "name": "gdp",
                  "subscripts": [],
                  "ast": {
                    "kind": "EQUATION",
                    "expression": "productivity * (capital_stock ^ capital_elasticity) * (labor_force ^ labor_elasticity)"

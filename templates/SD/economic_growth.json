                  "subtype": "Normal",
                  "name": "labor_force",
                  "subscripts": [],
                  "ast": {
                    "kind": "CONSTANT",
                    "expression": "100"
                  }
                },
                {
                  "type": "Auxiliary",
                  "subtype": "Normal",
                  "name": "capital_elasticity",
                  "subscripts": [],
                  "ast": {
                    "kind": "CONSTANT",
                    "expression": "0.3"
                  }
                },
                {
                  "type": "Auxiliary",
                  "subtype": "Normal",
                  "name": "labor_elasticity",
                  "subscripts": [],
                  "ast": {
                    "kind": "CONSTANT",

                  }
                },
                {
                  "type": "Auxiliary",
                  "subtype": "Normal",
                  "name": "savings_rate",
                  "subscripts": [],
                  "ast": {
                    "kind": "CONSTANT",
                    "expression": "0.2"
                  }
                },
                {
                  "type": "Auxiliary",
                  "subtype": "Normal",
                  "name": "depreciation_fraction",
                  "subscripts": [],
